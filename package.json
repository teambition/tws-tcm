{
  "name": "tws-tcm",
  "version": "1.2.0",
  "description": "Node.js SDK of TWS (Teambition Web Service) cloud messaging service.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "rm -rf build && tsc",
    "docs": "rm -rf docs && typedoc --out docs && touch docs/.nojekyll",
